{
	"name": "supabase-dashboard-sample",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"lint": "biome lint --write ./app/**/* ./components/**/* ./lib/**/*",
		"format": "biome format --write ./app/**/* ./components/**/* ./lib/**/*",
		"check": "biome check --write ./app/**/* ./components/**/* ./lib/**/*",
		"typecheck": "tsc --noEmit",
		"start": "next start",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"db": "supabase start",
		"db:reset": "npm run build:seed && supabase db reset",
		"db:reset:force": "supabase db reset",
		"build:seed": "cat supabase/seeds/*.sql > supabase/seed.sql",
		"migrate": "supabase db push",
		"func": "supabase function deploy"
	},
	"dependencies": {
		"@akfm/test-utils": "^0.1.1",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tooltip": "^1.2.7",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.52.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.525.0",
		"next": "15.4.1",
		"next-themes": "^0.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "4.7.0",
		"jsdom": "26.1.0",
		"lefthook": "1.12.2",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5",
		"vitest": "3.2.4"
	}
}
